				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	-- Input comes as an alias, convert directly to POSIX path	set pdfPath to POSIX path of input		-- Extract parent folder path	set parentFolder to do shell script "dirname " &amp; quoted form of pdfPath		-- Extract filename without extension and replace spaces with underscores	set pdfName to do shell script "basename " &amp; quoted form of pdfPath &amp; " .pdf | tr ' ' '_'"		-- Combine parent folder and new folder name	set folderPath to parentFolder &amp; "/" &amp; pdfName		-- Create the folder	do shell script "mkdir -p " &amp; quoted form of folderPath		-- Return the new folder path	return folderPathend run</string>
				</dict>
